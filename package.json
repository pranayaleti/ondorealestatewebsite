{
  "name": "ondo-real-estate",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://pranayaleti.github.io/ondorealestate",
  "scripts": {
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules/.cache out",
    "build": "next build",
    "build:full": "npm run optimize:images && npm run optimize:all-images && next build",
    "postbuild": "next-sitemap && npm run copy:headers",
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "lint": "next lint",
    "start": "next start",
    "sitemap": "next-sitemap",
    "predeploy": "npm run build:full && cp CNAME out/ && touch out/.nojekyll",
    "deploy": "gh-pages -d out",
    "export": "next build",
    "optimize:images": "node scripts/optimize-images.mjs",
    "optimize:all-images": "node scripts/optimize-all-images.mjs",
    "copy:headers": "cp public/_headers out/_headers 2>/dev/null || true",
    "analyze:bundle": "node scripts/analyze-bundle.mjs",
    "check:unused": "node scripts/check-unused-deps.mjs"
  },
